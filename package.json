{
  "name": "vue-virtual-tree",
  "version": "4.0.7",
  "description": "Tree component for large amount of data, base on Vue3",
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --dest lib --target lib ./src/components/index.ts && npm run build:types",
    "build:types": "tsc --project src/components/tsconfig.json"
  },
  "main": "./lib/vue-virtual-tree.common.js",
  "typings": "lib/index.d.ts",
  "files": ["lib"],
  "dependencies": {
    "core-js": "^3.6.5",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.4",
    "@vue/babel-plugin-jsx": "^1.0.3",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "ant-design-vue": "^2.1.2",
    "del": "^6.0.0",
    "express": "^4.17.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-resources-loader": "^1.4.1",
    "ts-import-plugin": "^1.6.7",
    "typescript": "~4.1.5",
    "vue-cli-plugin-style-resources-loader": "~0.1.5"
  },
  "keywords": [
    "tree component",
    "vue-next"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lycHub/vue-virtual-tree.git"
  },
  "homepage": "https://lychub.github.io/vue-virtual-tree",
